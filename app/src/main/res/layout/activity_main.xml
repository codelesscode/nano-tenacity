        android:layout_alignStart="@+id/btnMyOwnApp"
        android:layout_x="63dp"
        android:layout_y="237dp" />

    <Button
        android:layout_width="205dp"
        android:layout_height="wrap_content"
        android:text="BACON READER"
        android:background="#F08C35"
        android:id="@+id/btnBaconReader"
        android:layout_below="@+id/btnBuildItBigger"
        android:layout_marginTop="10dp"
        android:layout_alignLeft="@+id/btnBuildItBigger"
        android:layout_alignStart="@+id/btnBuildItBigger"
        android:layout_alignRight="@+id/btnBuildItBigger"
        android:layout_alignEnd="@+id/btnBuildItBigger"
        android:layout_x="63dp"
        android:layout_y="298dp" />

    <Button
        android:layout_width="205dp"
        android:layout_height="wrap_content"
        android:text="CAPSTONE: MY OWN APP"
        android:id="@+id/btnMyOwnApp"
        android:background="#fff0142a"
        android:layout_below="@+id/btnBaconReader"
        android:layout_marginTop="10dp"
        android:layout_alignRight="@+id/textView"
        android:layout_alignEnd="@+id/textView"
        android:layout_x="63dp"
        android:layout_y="357dp" />
